    "license": "MPL-2.0",
    "version": "1.0.1",
    "exposed-modules": [
        "Scene3d",
        "Scene3d.Material",
        "Scene3d.Mesh",
        "Scene3d.Mesh.Encode",
        "Scene3d.Mesh.Decode",
